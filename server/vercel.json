{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "env": {
      "MY_API_KEY": "@my-api-key",
      "DATABASE_URL": "@database-url"
    },
    "github": {
      "enabled": true
    }
  }
  